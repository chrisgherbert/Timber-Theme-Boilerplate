{% block html_head_container %}

<!doctype html>
<html class="no-js" {{site.language_attributes}}>
<head>
	{{ site_options_tracking.after_opening_head }}
	<meta charset="{{site.charset}}">
	<meta name="description" content="{{site.description}}">
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="pingback" href="{{site.pingback_url}}">

	{% include 'head/favicons.twig' %}

	{{function('wp_head')}}

	{% block head %}
	{% endblock %}

	{{ site_options_tracking.before_closing_head }}

	</head>

{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">
		{{ site_options_tracking.after_opening_body }}

		{% include 'components/mobile-menu.twig' %}

		{% block header_container %}
			{% include 'header/header.twig' %}
		{% endblock %}

		<main id="main" role="main" class="main">

			{% block content %}
				Sorry, nothing here.
			{% endblock %}

		</main>

		{% block footer_container %}

			{% include 'footer/footer.twig' %}

			{{ function('wp_footer') }}

		{% endblock %}

		{{ site_options_tracking.before_closing_body }}
	</body>
</html>